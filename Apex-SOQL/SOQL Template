--template 1 using AND
SELECT Borrower_First_Name__c,
Convo_Had_Closing_Disclosure_Check_In__c,
Closing_Disclosure_Sent_Date__c,
Initial_CD_Signed_Date__c,
Initial_CD_Status__c,
Loan_Advisor__c,
StageName,
Processing_Stage__c 
FROM Opportunity WHERE StageName IN ('Processing', 'Closing') 
AND Initial_CD_Status__c in ('Sent','Signed') AND Processing_Stage__c not in ('Signed') 
AND Convo_Had_Closing_Disclosure_Check_In__c not in ('Yes')

--template 2 using OR
SELECT Closer__c,
Final_Approval_Date__c,
Processor__c,
StageName,
Processing_Stage__c 
FROM Opportunity WHERE StageName in ('Processing') 
OR (Processing_Stage__c in ('Clear to Close'))
