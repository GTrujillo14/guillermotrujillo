Dim sRange As Range
    Dim lastRow As Long, sRow As Long, dRow As Long
    Dim Column As Range
    Dim dst As Worksheet, src As Worksheet
    Dim sCount As Long
    
Set dst = Worksheets("Sheet2") 'Destination Worksheet
Set src = Worksheets("Sheet1") 'Source Worksheet
sRow = 2 'Starting source row
dRow = 1 'Starting destination row
lastRow = src.Cells(src.Rows.Count, "A").End(xlUp).Row
 
dst.Activate
dst.Range("1:50").ClearContents
src.Activate
    sCount = 0
        For sRow = sRow To lastRow
        If sCount > 49 Then Exit For 'Stop loop at number of desired results
                If Cells(sRow, 5).Value = "criteria1" Then
                src.Rows(sRow).Select 'Select entire row
                Selection.Copy Destination:=dst.Rows(dRow) 'copy to destination worksheet
                sCount = sCount + 1
                dRow = dRow + Selection.Rows.Count 'Move down by number of rows in selection
                    ElseIf Cells(sRow, 5).Value = "criteria2" Then
                    src.Rows(sRow).Select 'Select entire row
                    Selection.Copy Destination:=dst.Rows(dRow) 'copy to destination worksheet
                    sCount = sCount + 1
                    dRow = dRow + Selection.Rows.Count 'Move down by number of rows in selection
                        ElseIf Cells(sRow, 5).Value = "criteria3" Then
                        src.Rows(sRow).Select 'Select entire row
                        Selection.Copy Destination:=dst.Rows(dRow) 'copy to destination worksheet
                        sCount = sCount + 1
                        dRow = dRow + Selection.Rows.Count 'Move down by number of rows in selection
                            ElseIf Cells(sRow, 5).Value = "criteria4" Then
                        src.Rows(sRow).Select 'Select entire row
                        Selection.Copy Destination:=dst.Rows(dRow) 'copy to destination worksheet
                        sCount = sCount + 1
                        dRow = dRow + Selection.Rows.Count 'Move down by number of rows in selection
                             ElseIf Cells(sRow, 5).Value = "criteria5" Then
                            src.Rows(sRow).Select 'Select entire row
                            Selection.Copy Destination:=dst.Rows(dRow) 'copy to destination worksheet
                            sCount = sCount + 1
                            dRow = dRow + Selection.Rows.Count 'Move down by number of rows in selection
                            src.Range("A1").Select
     
     End If
 
 Next sRow
 
 MsgBox sCount & " Calls Selected: Transfer Done"
 
 Set src = Nothing
 Set dst = Nothing
 
